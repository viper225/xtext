/*
 * generated by Xtext
 */
grammar DebugInternalRegionAccessTestLanguage ;

// Rule Root
ruleRoot :
	'test' (
		ruleSimple |
		ruleDelegation |
		ruleUnassigned |
		rulePrefixedUnassigned |
		'5' ruleExpression
	)
;

// Rule Simple
ruleSimple :
	'1' RULE_ID
;

// Rule Delegation
ruleDelegation :
	'2' ruleDelegate
;

// Rule Unassigned
ruleUnassigned :
	'3' ruleDelegate
;

// Rule PrefixedUnassigned
rulePrefixedUnassigned :
	'4' rulePrefixedDelegate
;

// Rule PrefixedDelegate
rulePrefixedDelegate :
	'prefix' ruleDelegate
;

// Rule Delegate
ruleDelegate :
	RULE_ID
;

// Rule Expression
ruleExpression :
	rulePrimary (
		'+' rulePrimary
	)*
;

// Rule Primary
rulePrimary :
	RULE_ID |
	ruleParenthesized
;

// Rule Parenthesized
ruleParenthesized :
	'(' ruleExpression ')'
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' {skip();}
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? {skip();}
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ {skip();}
;

RULE_ANY_OTHER :
	.
;